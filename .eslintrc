{
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": ["react"],
  "env": {
    "jasmine": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "builtin": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [2, "last"],
    "curly": [ 2, "all" ],
    "dot-notation": [2, { "allowKeywords": true }],
    "eqeqeq": [2, "allow-null"],
    "strict": [2, "global"],
    "guard-for-in": 2,
    "max-depth": [2, 3],
    "max-len": [2, 100, 2],
    "new-cap": [2, {"capIsNewExceptions": ["Class"]}],
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-with": 2,
    "quotes": [2, "single"],
    "semi": [0, "never"],
    "strict": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"],
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "react/jsx-sort-props": 2,
    "react/jsx-uses-vars": 2,
    "react/wrap-multilines": 2
  }
}
